{
  "$schema": "./node_modules/oxlint/configuration_schema.json",
  "plugins": ["oxc"],
  "env": {
    "browser": true
  },
  "categories": {
    "correctness": "error"
  },
  "rules": {
    "quotes": ["warn", "single"],
    "semi": ["warn", "never"],
    "vue/multi-word-component-names": ["warn", { "ignores": ["index"] }],
    "vue/no-setup-props-destructure": "off",
  }
}